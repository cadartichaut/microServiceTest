{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "Test Exercise API",
    "description": "Fidelity micro-service. All routes require a [JSON Web Token](https://jwt.io/) authentication to work. All routes require app_platform and app_version headers to work."
  },
  "basePath": "/",
  "host": "localhost",
  "schemes": [
    "http",
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json; charset=utf-8"
  ],
  "paths": {
    "/hello/{userName}/{age}": {
      "x-swagger-router-controller": "hello",
      "get": {
        "description": "Say Hello.",
        "operationId": "helloWorld",
        "parameters": [
          {
            "name": "userName",
            "required": true,
            "type": "string",
            "in": "path"
          },
          {
            "name": "age",
            "required": true,
            "type": "number",
            "in": "path"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "type": "object",
              "required": [
                "greetings"
              ],
              "properties": {
                "greetings": {
                  "type": "string",
                  "description": "How we should greet our new friend"
                }
              }
            }
          }
        }
      }
    },
    "/earn/{userId}/{points}": {
      "x-swagger-router-controller": "points",
      "get": {
        "description": "",
        "operationId": "earnPoints",
        "parameters": [
          {
            "name": "userId",
            "required": true,
            "type": "number",
            "in": "path"
          },
          {
            "name": "points",
            "required": true,
            "type": "number",
            "in": "path"
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/checkPoints/{userId}}": {
      "x-swagger-router-controller": "points",
      "get": {
        "description": "",
        "operationId": "checkPoints",
        "parameters": [
          {
            "name": "userId",
            "required": true,
            "type": "number",
            "in": "path"
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }

  }
}